#include <stdio.h>

int product(int a, int b, int c)
{
    return a * b * c;
}


int main()
{
   printf("%d\n", product(1,2,3));

    int points[5] = {60, 50, 75, 100, 95};
    int i = 0;
    int x = 0;
    for(i = 0; i < 5; i++)
    {
        x = points[i] + 1;
        printf("%d", x);
    }

    return 0;
}
